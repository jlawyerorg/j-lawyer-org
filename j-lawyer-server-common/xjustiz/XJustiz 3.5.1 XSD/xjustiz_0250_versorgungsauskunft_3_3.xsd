                     </xs:annotation>
                  </xs:element>
                  <xs:element name="internationaleWertpapierkennnummer"
                              minOccurs="0"
                              type="tns:Type.VAG.ISIN">
                     <xs:annotation>
                        <xs:documentation>"International Securities Identification Number" (ISIN) gemäß Norm ISO 6166 (zwölfstellige Buchstaben-Zahlen-Kombination)</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:sequence>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:simpleType name="Type.VAG.ISIN">
      <xs:restriction base="din91379:datatypeC">
         <xs:pattern value="[a-z|A-Z]{2}[a-z|A-Z|0-9]{9}\d"/>
      </xs:restriction>
   </xs:simpleType>
   <xs:complexType name="Type.VAG.Kurzmitteilung">
