                                    <xs:element name="sonstigerZeitpunkt" type="din91379:datatypeC"/>
                                 </xs:choice>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="auswahl_zinsende">
                              <xs:complexType>
                                 <xs:choice>
                                    <xs:element name="zinsende.rechtskraft" fixed="true" type="xs:boolean">
                                       <xs:annotation>
                                          <xs:documentation>Das Datum des Zinsendes ist das Datum der Rechtskraft.</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="bisZum" type="xs:date"/>
                                 </xs:choice>
                              </xs:complexType>
                           </xs:element>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="tarif" minOccurs="0" type="din91379:datatypeC">
