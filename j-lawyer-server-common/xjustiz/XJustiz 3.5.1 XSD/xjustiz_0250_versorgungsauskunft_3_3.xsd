                                 <xs:choice>
                                    <xs:element name="zinssatzInProzent" type="tns:Type.VAG.Decimal.4_2">
                                       <xs:annotation>
                                          <xs:documentation>Zinsatz in %</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                    <xs:element name="prozentUeberBasiszinssatz" type="tns:Type.VAG.Decimal.4_2">
                                       <xs:annotation>
                                          <xs:documentation>% über Basiszinssatz</xs:documentation>
                                       </xs:annotation>
                                    </xs:element>
                                 </xs:choice>
                              </xs:complexType>
                           </xs:element>
                           <xs:element name="kapitalbetrag" minOccurs="0" type="tns:Type.VAG.Decimal.9_2">
                              <xs:annotation>
                                 <xs:documentation>Hier wird ein Betrag für die Zinsberechnung eingegeben, wenn dieser abweichend vom Ausgleichsbetrag ist.</xs:documentation>
                              </xs:annotation>
