<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema"
           xmlns:tns="http://www.xjustiz.de"
           xmlns:din91379="urn:xoev-de:kosit:xoev:datentyp:din-91379_2022-08"
           targetNamespace="http://www.xjustiz.de"
           version="3.2.0"
           elementFormDefault="qualified"
           attributeFormDefault="unqualified">
   <xs:annotation>
      <xs:appinfo>
         <standard>
            <nameLang>Strukturierte Fachdaten für die Kommunikation im elektronischen Rechtsverkehr</nameLang>
            <nameKurz>XJustiz</nameKurz>
            <nameTechnisch>xjustiz</nameTechnisch>
            <kennung>urn:xoev-de:blk-ag-it-standards:standard:xjustiz</kennung>
            <beschreibung>XJustiz ist der bundesweit einheitliche Standard für den Austausch strukturierter elektronischer Informationen mit der Justiz.</beschreibung>
         </standard>
         <versionStandard>
            <version>3.5.1</version>
            <beschreibung>XJustiz beschreibt ein standardisiertes Datenaustauschformat für die elektronische Kommunikation innerhalb und mit der Justizverwaltung.</beschreibung>
            <versionXOEVProfil>1.7.2</versionXOEVProfil>
            <versionXOEVHandbuch>2.3.1</versionXOEVHandbuch>
            <versionXGenerator>3.1.1</versionXGenerator>
            <versionModellierungswerkzeug>19.0 SP3</versionModellierungswerkzeug>
            <nameModellierungswerkzeug>MagicDraw</nameModellierungswerkzeug>
         </versionStandard>
      </xs:appinfo>
   </xs:annotation>
   <xs:include schemaLocation="xjustiz_0000_grunddatensatz_3_5.xsd"/>
   <xs:include schemaLocation="xjustiz_2910_cl_dabag_3_1.xsd"/>
   <xs:import schemaLocation="din-norm-91379-datatypes.xsd"
              namespace="urn:xoev-de:kosit:xoev:datentyp:din-91379_2022-08"/>
   <xs:complexType name="Type.DABAG.Abtretung.Berechtigter">
      <xs:sequence>
         <xs:element name="grund" minOccurs="0" type="tns:Code.DABAG.Abtretungsgrund"/>
         <xs:element name="gegenstand" minOccurs="0" type="din91379:datatypeC"/>
         <xs:element name="zusatz" minOccurs="0" type="din91379:datatypeC"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.DABAG.Aenderungsvormerkung">
      <xs:sequence>
         <xs:element name="gegenstand" minOccurs="0" type="din91379:datatypeC"/>
         <xs:element name="berechtigter"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.GDS.Ref.Rollennummer"/>
         <xs:element name="dinglichBerechtigter"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.DinglichBerechtigter"/>
         <xs:element name="eintragungsgrundlage"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Ref.Eintragungsgrundlage"/>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.DABAG.Anteil">
      <xs:sequence>
         <xs:element name="auswahl_anteil">
            <xs:complexType>
               <xs:choice>
                  <xs:element name="anteilNumerisch">
                     <xs:complexType>
                        <xs:sequence>
                           <xs:element name="zaehler" type="xs:integer"/>
                           <xs:element name="nenner" type="xs:integer"/>
                        </xs:sequence>
                     </xs:complexType>
                  </xs:element>
                  <xs:element name="anteilSonstiges" type="din91379:datatypeC">
                     <xs:annotation>
                        <xs:documentation>Bestimmt die Größe des Anteils, wenn diese nicht in Form eines Bruchs (zaehler/nenner) angegeben werden kann.</xs:documentation>
                     </xs:annotation>
                  </xs:element>
               </xs:choice>
            </xs:complexType>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.DABAG.Belastung">
      <xs:sequence>
         <xs:element name="lfdNummer" minOccurs="0" type="din91379:datatypeC"/>
         <xs:element name="istVormerkung" minOccurs="0" type="xs:boolean"/>
         <xs:element name="vormerkungsinhalt" minOccurs="0" type="din91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Gibt die Vormerkungsvorschrift an und ob es sich um eine Vormerkung für ein Recht oder eine Vormerkung handelt (z. B. Vormerkung gemäß § 18 Abs. 2 GBO auf Eintragung folgenden Rechts oder Vormerkung gemäß § 18 Abs. 2 GBO: Vormerkung auf Eintragung folgenden Rechts).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="belasteteBuchungsstelle"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Ref.Buchungsstelle"/>
         <xs:element name="teilflaechenbelastung"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Teilflaechenbelastung"/>
         <xs:element name="belasteterEigentuemer"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Ref.Eigentuemer">
            <xs:annotation>
               <xs:documentation>Referenz auf die belasteten Eigentümer. Nur relevant, wenn eine belastete Buchungsstelle mehrere Eigentümer hat (es mehrere Miteigentumsanteile gibt) und die Belastung nicht alle Eigentümer betrifft.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="berechtigter"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Berechtigter"/>
         <xs:element name="rangvorbehalt"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Rangvorbehalt"/>
         <xs:element name="bedingt" minOccurs="0" type="xs:boolean"/>
         <xs:element name="befristet" minOccurs="0" type="xs:boolean"/>
         <xs:element name="abtretungsausschluss" minOccurs="0" type="din91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Gibt an, in welcher Form die Abtretung/Pfändung der Belastung beschränkt ist (z. B. bedingt abtretbar, bedingt verpfändbar, nicht abtretbar, nicht verpfändbar, nicht abtretbar und nicht verpfändbar; auch Kombination von Werten möglich).</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="vorloeschklausel" minOccurs="0" type="din91379:datatypeC"/>
         <xs:element name="verfuegungsbeschraenkung"
                     minOccurs="0"
                     type="din91379:datatypeC"/>
         <xs:element name="zusatz" minOccurs="0" type="din91379:datatypeC"/>
         <xs:element name="widerspruchBelastung"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Widerspruch.Belastung"/>
         <xs:element name="eintragungsgrundlage"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Ref.Eintragungsgrundlage"/>
         <xs:element name="bundeslandfremdesGrundbuchblatt"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.BundeslandfremdesGrundbuchblatt"/>
         <xs:element name="aenderungsvormerkung"
                     minOccurs="0"
                     maxOccurs="unbounded"
                     type="tns:Type.DABAG.Aenderungsvormerkung"/>
      </xs:sequence>
      <xs:attribute name="id" type="din91379:datatypeC" use="required"/>
   </xs:complexType>
   <xs:complexType name="Type.DABAG.Belastung.Abt2">
      <xs:complexContent>
         <xs:extension base="tns:Type.DABAG.Belastung">
            <xs:sequence>
               <xs:element name="typ" minOccurs="0" type="tns:Code.DABAG.Belastungstyp.Abt2">
                  <xs:annotation>
                     <xs:documentation>Typ der Belastung (Lasten und Beschränkungen, z. B. Erwerbsvormerkung, Grunddienstbarkeit, Reallast, Verfügungsbeschränkung).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="art" minOccurs="0" type="din91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Art der Belastung (typabhängig). Für die Typen Altenteil, Erbbaurecht und Vorkaufsrecht die Angabe, welche juristische Bezeichnung im Grundbuch eingetragen wird (z. B. Leibgeding, Erbbaurecht, Wiederkaufsrecht gemäß § 20 RSG).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="inhalt" minOccurs="0" type="din91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Inhalt der Belastung (typabhängig). Relevant für die Typen Belastung1010, Dauerwohnrecht, Dienstbarkeit, Erwerbsvormerkung, Nutzungsrecht, Reallast und Verfügungsbeschränkung. Pflichtangabe für die Typen Belastung1010, Dauerwohnrecht, Dienstbarkeit, Nutzungsrecht, Reallast und Verfügungsbeschränkung.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="quote" minOccurs="0" type="din91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Höhe des Nießbrauchs an einer Buchungsstelle bei ungeteiltem Alleineigentum oder Miteigentumsanteil. Nur relevant für Belastungen des Typs 'Nießbrauch'.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="belastungAllerAnteile" minOccurs="0" type="xs:boolean">
                  <xs:annotation>
                     <xs:documentation>Gibt an, ob alle Miteigentumsanteile der Buchungsstelle belastet sind (true) oder nur einzelne Miteigentumsanteile der Buchungsstelle (false). Nur relevant für Belastungen des Typs 'Belastung nach § 1010'.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="veraeusserungsbeschraenkung" minOccurs="0" type="xs:boolean">
                  <xs:annotation>
                     <xs:documentation>Gibt an, ob eine Veräußerungsbeschränkung gemäß § 35 WEG vorliegt (true) oder nicht (false). Nur relevant für Belastungen des Typs 'Dauerwohnrecht'.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="ausuebungsbeschraenkung" minOccurs="0" type="xs:boolean">
                  <xs:annotation>
                     <xs:documentation>Gibt an, ob die Ausübung des Erbbaurechts auf einen Teil der Buchungsstelle beschränkt ist (true) oder nicht (false). Nur relevant für Belastungen des Typs 'Erbbaurecht'.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="betrag" minOccurs="0" type="tns:Type.GDS.Geldbetrag">
                  <xs:annotation>
                     <xs:documentation>Betrag eines Erbbauzinses. Nur relevant für Belastungen des Typs 'Reallast', Art 'Erbbauzins'.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="intervall" minOccurs="0" type="din91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Intervall eines Erbbauzinses. Nur relevant für Belastungen des Typs 'Reallast', Art 'Erbbauzins'.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="dinglichBerechtigter"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="tns:Type.DABAG.DinglichBerechtigter"/>
               <xs:element name="berechtigungsverhaeltnis"
                           minOccurs="0"
                           type="din91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Bezeichnung des Rechtsverhältnisses zweier oder mehr dinglich Berechtigter, deren Anteile nicht in Bruchteilen angegeben sind (Gesamtberechtigte gemäß § 428 BGB oder Mitberechtigte gemäß § 432 BGB).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="beguenstigterEigentuemer"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="tns:Type.DABAG.Ref.Eigentuemer">
                  <xs:annotation>
                     <xs:documentation>Referenz auf die begünstigten Eigentümer. Nur relevant für Vorkaufsrechte und Belastungen nach § 1010 BGB.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="erbbaurecht"
                           minOccurs="0"
                           type="tns:Type.DABAG.Ref.Buchungsstelle">
                  <xs:annotation>
                     <xs:documentation>Zugehörige Buchungsstelle im Erbbaugrundbuch, Untererbbaugrundbuch bzw. Stavengrundbuch. Nur relevant für Belastungen des Typs 'Erbbaurecht'.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="nutzungsrecht"
                           minOccurs="0"
                           type="tns:Type.DABAG.Ref.Buchungsstelle">
                  <xs:annotation>
                     <xs:documentation>Zugehörige Buchungsstelle im Nutzungsrechtsgrundbuch bzw. Gebäudegrundbuch. Nur relevant für Belastungen des Typs 'Nutzungsrecht'.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="pfaendung" minOccurs="0" type="tns:Type.DABAG.Pfaendung">
                  <xs:annotation>
                     <xs:documentation>Verfügungsbeschränkung vom Typ Pfändung</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="nacherbfolge"
                           minOccurs="0"
                           type="tns:Type.DABAG.Nacherbfolge">
                  <xs:annotation>
                     <xs:documentation>Verfügungsbeschränkung vom Typ Nacherbfolge</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="belastungsform"
                           minOccurs="0"
                           type="tns:Code.DABAG.Belastungsform"/>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Type.DABAG.Belastung.Abt3">
      <xs:complexContent>
         <xs:extension base="tns:Type.DABAG.Belastung">
            <xs:sequence>
               <xs:element name="art" minOccurs="0" type="tns:Code.DABAG.Belastungstyp.Abt3">
                  <xs:annotation>
                     <xs:documentation>Art des Grundpfandrechts (z. B. Grundschuld oder Hypothek)</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="buchgrundschuld" minOccurs="0" type="xs:boolean">
                  <xs:annotation>
                     <xs:documentation>Kennzeichnet, ob es sich bei dem Grundpfandrecht um eine Buchgrundschuld (true) oder eine Briefgrundschuld (false) handelt. Der Wert "true" entspricht der Eintragung "ohne Brief". Hinweis: Bei einer Arresthypothek, Sicherungshypothek, Zwangssicherungshypothek hat das Attribut immer den Wert "true", da es sich immer um eine Buchgrundschuld handelt. In diesem Fall entfällt der Zusatz "ohne Brief".</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="betrag" minOccurs="0" type="tns:Type.GDS.Geldbetrag">
                  <xs:annotation>
                     <xs:documentation>Betrag des Grundpfandrechts</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="betragP10ZVG" minOccurs="0" type="tns:Type.GDS.Geldbetrag">
                  <xs:annotation>
                     <xs:documentation>Gibt an, in welcher Höhe die Zwangshypothek aufschiebend bedingt ist. Pflichtangabe, wenn die Hypothek teilweise aufschiebend bedingt ist.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="auswahl_aufschiebendeBedingungP10ZVG" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>Gibt an, ob die Zwangshypothek vollständig oder teilweise aufschiebend bedingt ist.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:choice>
                        <xs:element name="vollstaendig"
                                    fixed="die Hypothek ist insoweit dadurch bedingt, dass ein der Forderung nach § 10 Abs. 1 Nr. 3 ZVG zustehendes Vorrecht vor Erlöschen der Forderung wegfällt"
                                    type="din91379:datatypeC"/>
                        <xs:element name="teilweise"
                                    fixed="in Höhe eines Teilbetrages ist die Hypothek dadurch bedingt, dass das der Forderung nach § 10 Abs. 1 Nr. 3 ZVG zustehende Vorrecht vor Erlöschen der Forderung wegfällt"
                                    type="din91379:datatypeC"/>
                     </xs:choice>
                  </xs:complexType>
               </xs:element>
               <xs:element name="auswahl_ausschluss1179a" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>Gibt an, ob der Löschungsanspruch gemäß § 1179a BGB teilweise oder vollständig ausgeschlossen ist; keine Angabe bedeutet, dass der Löschungsanspruch gemäß § 1179a BGB nicht ausgeschlossen ist. Die vom Ausschluss des Löschungsanspruchs begünstigten Grundpfandrechte sind unter "beguenstigtesRecht1179a" angegeben.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:choice>
                        <xs:element name="vollstaendig"
                                    fixed="Ausschluss des gesetzlichen Löschungsanspruchs gemäß § 1179a BGB gegenüber Recht"
                                    type="din91379:datatypeC"/>
                        <xs:element name="teilweise"
                                    fixed="Ausschluss des gesetzlichen Löschungsanspruchs gemäß § 1179a BGB teilweise gegenüber Recht"
                                    type="din91379:datatypeC"/>
                     </xs:choice>
                  </xs:complexType>
               </xs:element>
               <xs:element name="auswahl_ausschluss1179b" minOccurs="0">
                  <xs:annotation>
                     <xs:documentation>Gibt an, ob der Löschungsanspruch gemäß § 1179b BGB teilweise oder vollständig ausgeschlossen ist; keine Angabe bedeutet, dass der Löschungsanspruch gemäß § 1179b BGB nicht ausgeschlossen ist.</xs:documentation>
                  </xs:annotation>
                  <xs:complexType>
                     <xs:choice>
                        <xs:element name="vollstaendig"
                                    fixed="Ausschluss des gesetzlichen Löschungsanspruchs gemäß § 1179b BGB gegenüber dem Recht selbst"
                                    type="din91379:datatypeC"/>
                        <xs:element name="teilweise"
                                    fixed="Ausschluss des gesetzlichen Löschungsanspruchs gemäß § 1179b BGB teilweise gegenüber dem Recht selbst"
                                    type="din91379:datatypeC"/>
                     </xs:choice>
                  </xs:complexType>
               </xs:element>
               <xs:element name="eintragungsgrund" minOccurs="0" type="din91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Gibt bei Zwangshypotheken den Eintragungsgrund an (z. B. im Verwaltungszwangsverfahren).</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="zahlungsbedingung" minOccurs="0" type="din91379:datatypeC">
                  <xs:annotation>
                     <xs:documentation>Gibt bei Rentenschulden die Zahlungsbedingungen an.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="besondererGlaeubiger" minOccurs="0" type="din91379:datatypeD">
                  <xs:annotation>
                     <xs:documentation>Angabe bei Wertpapierhypotheken (z. B. den jeweiligen Inhaber der Schuldverschreibung)</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="zins"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="tns:Type.DABAG.Zins"/>
               <xs:element name="nebenleistung"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="tns:Type.DABAG.Nebenleistung"/>
               <xs:element name="vollstreckbarkeit"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="tns:Type.DABAG.Vollstreckbarkeit"/>
               <xs:element name="grundbuchvertreter"
                           minOccurs="0"
                           type="tns:Type.GDS.Ref.Rollennummer">
                  <xs:annotation>
                     <xs:documentation>Der Grundbuchvertreter wird durch die Angabe einer natürlichen oder juristischen Person bestimmt.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="beguenstigtesRecht1179a"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="tns:Type.DABAG.Identifikation.Belastung">
                  <xs:annotation>
                     <xs:documentation>Rechte, denen gegenüber der Ausschluss des gesetzlichen Löschungsanspruch gemäß § 1179a BGB erklärt wurde. Nur relevant, bei Ausschluss gemäß § 1179a BGB.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Type.DABAG.Berechtigter">
      <xs:complexContent>
         <xs:extension base="tns:Type.DABAG.Eintragungsbeteiligter">
            <xs:sequence>
               <xs:element name="anteil" minOccurs="0" type="tns:Type.DABAG.Anteil">
                  <xs:annotation>
                     <xs:documentation>Anteil muss nur angegeben werden, wenn mehrere Berechtigte vorhanden sind.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="verfuegungsbeschraenkungBerechtigter"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="tns:Type.DABAG.Verfuegungsbeschraenkung.Berechtigter">
                  <xs:annotation>
                     <xs:documentation>Anteil muss nur angegeben werden, wenn mehrere Berechtigte vorhanden sind.</xs:documentation>
                  </xs:annotation>
               </xs:element>
               <xs:element name="niessbrauchBerechtigter"
                           minOccurs="0"
                           maxOccurs="unbounded"
                           type="tns:Type.DABAG.Niessbrauch.Berechtigter">
                  <xs:annotation>
                     <xs:documentation>Anteil muss nur angegeben werden, wenn mehrere Berechtigte vorhanden sind.</xs:documentation>
                  </xs:annotation>
               </xs:element>
            </xs:sequence>
         </xs:extension>
      </xs:complexContent>
   </xs:complexType>
   <xs:complexType name="Type.DABAG.Beteiligter.Grundbuchdaten">
      <xs:sequence>
         <xs:element name="referenzierungBeteiligter" type="tns:Type.GDS.Ref.Rollennummer"/>
         <xs:element name="wohnort" minOccurs="0" type="din91379:datatypeB"/>
         <xs:element name="amtstitelNotar" minOccurs="0" type="din91379:datatypeC"/>
         <xs:element name="amtstaetigkeitenId" minOccurs="0" type="din91379:datatypeC">
            <xs:annotation>
               <xs:documentation>Fachlicher Schlüssel eines Notars im Notarverzeichnis. Wird benötigt, um einen Notar eindeutig über die WS-Schnittstelle im NVZ zu identifizieren.</xs:documentation>
            </xs:annotation>
         </xs:element>
         <xs:element name="zusatzP15IIGBV" minOccurs="0" type="din91379:datatypeC"/>
         <xs:element name="eintragungsname" minOccurs="0" type="din91379:datatypeD">
            <xs:annotation>
               <xs:documentation>Name, der abweichend von der Registereintragung im Grundbuch eingetragen ist.</xs:documentation>
            </xs:annotation>
         </xs:element>
      </xs:sequence>
   </xs:complexType>
   <xs:complexType name="Type.DABAG.Buchungsstelle">
      <xs:sequence>
         <xs:element name="lfdNummer" minOccurs="0" type="din91379:datatypeC"/>
         <xs:element name="auswahl_buchungsstelle">
            <xs:complexType>
               <xs:choice>
                  <xs:element name="grundstueck" type="tns:Type.DABAG.Grundstueck"/>
                  <xs:element name="erbbaurecht" type="tns:Type.DABAG.Erbbaurecht"/>
                  <xs:element name="nutzungsrecht" type="tns:Type.DABAG.Nutzungsrecht"/>
                  <xs:element name="wegEinheit" type="tns:Type.DABAG.WEG.Einheit"/>
                  <xs:element name="miteigentumP3GBO" type="tns:Type.DABAG.MiteigentumP3GBO"/>
               </xs:choice>
            </xs:complexType>
         </xs:element>
